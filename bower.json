{
  "name": "umd-stdlib",
  "version": "1.0.0",
  "main": "**",
  "umd": true,
  "ignore": [
    ".*",
    "component.json",
    "package.json",
    "node_modules"
  ],
  "licenses": "MIT",
  "dependencies": {
    "async": "~1.4.2",
    "backbone": "~1.2.1",
    "backbone.validation": "~0.11.5",
    "bootstrap": "~3.3.5",
    "eventEmitter": "~4.2.11",
    "font-awesome": "~4.4.0",
    "handlebars": "~4.0.2",
    "jquery": "1.x",
    "jquery-colorbox": "~1.6.3",
    "jquery-mockjax": "~2.0.1",
    "jquery-mousewheel": "~3.1.13",
    "jquery-splendid-textchange": "git+https://github.com/spicyj/jquery-splendid-textchange.git",
    "jquery-toastmessage-plugin": "git+https://github.com/akquinet/jquery-toastmessage-plugin.git",
    "jquery-ui": "~1.11.4",
    "json3": "~3.3.2",
    "lodash": "~3.10.0",
    "moment": "~2.10.5",
    "qs": "git+https://github.com/hapijs/qs#~5.1.0"
  },
  "overrides": {
    "backbone": {
      "dependencies": {
        "jquery": "*",
        "lodash": "*"
      }
    },
    "backbone.validation": {
      "dependencies": {
        "backbone": "*"
      },
      "exports": "Backbone.Validation"
    },
    "bootstrap": {
      "main": [
        "dist/css/bootstrap.css",
        "dist/js/bootstrap.js",
        "dist/fonts/**"
      ],
      "exports": "$.fn.emulateTransitionEnd"
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.css",
        "fonts/**"
      ]
    },
    "handlebars": {
      "main": "handlebars.runtime.js"
    },
    "jquery-colorbox": {
      "main": [
        "jquery.colorbox.js",
        "example1/colorbox.css",
        "example1/**"
      ],
      "exports": "jQuery.colorbox"
    },
    "jquery-splendid-textchange": {
      "dependencies": {
        "jquery": "*"
      },
      "main": "jquery.splendid.textchange.js",
      "exports": "jQuery"
    },
    "jquery-toastmessage-plugin": {
      "dependencies": {
        "jquery": "*"
      },
      "main": [
        "src/main/javascript/jquery.toastmessage.js",
        "src/main/resources/css/jquery.toastmessage.css",
        "src/main/resources/images/**"
      ],
      "exports": "jQuery.fn.toastmessage"
    },
    "jquery-ui": {
      "main": [
        "ui/core.js",
        "ui/widget.js",
        "ui/position.js",
        "ui/menu.js",
        "ui/mouse.js",
        "ui/autocomplete.js",
        "ui/datepicker.js",
        "ui/i18n/datepicker-fr.js",
        "ui/draggable.js",
        "ui/effect.js",
        "ui/effect-blind.js",
        "ui/effect-drop.js",
        "ui/effect-slide.js",
        "ui/slider.js",
        "ui/sortable.js",
        "ui/tabs.js",
        "themes/redmond/jquery-ui.css",
        "themes/redmond/images/**"
      ]
    }
  }
}
